pipeline {
  agent {
    docker { image 'node:16-alpine' }
  }
  stages {
    stage('Clone Repository') {
      steps {
        git credentialsId: 'Githubaccess', url: 'https://github.com/KKDevOps07/Jenkins.git', branch: 'master'
      }
    }
    stage('Test') {
      steps {
        dir('docker-agents') {  // change this to the actual folder inside your repo if needed
          sh 'node --version'
        }
      }
    }
  }
}
